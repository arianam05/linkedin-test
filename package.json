{
  "name": "linkedin-test",
  "version": "1.0.0",
  "description": "Escenarios para testear linkedin",
  "author": "Ariana Mendez",
  "scripts": {
    "debug": "node --inspect=1337 --debug-brk --nolazy node_modules/cucumber/bin/cucumber-js --tags @debug --format json:./reports/report.json",
    "test": "./node_modules/.bin/cucumber-js --world-parameters \"{\\\"browser\\\": \\\"safari\\\"}\"",
    "test-report": "./node_modules/.bin/cucumber-js --format json:./reports/report.json",
    "test-chrome": "./node_modules/.bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"chrome\\\"}\"",
    "test-ie": "./node_modules/.bin/cucumber-js.  --world-parameters \"{\\\"browser\\\": \\\"ie\\\"}\"",
    "test-edge": "./node_modules/.bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"edge\\\"}\"",
    "test-firefox": "./node_modules/.bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"firefox\\\"}\"",
    "test-safari": "./node_modules/.bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"safari\\\"}\"",
    "test-chrome-report": "./node_modules/.bin/cucumber-js  --format json:./reports/report.json --world-parameters \"{\\\"browser\\\": \\\"chrome\\\"}\"",
    "test-ie-report": "./node_modules/.bin/cucumber-js  --format json:./reports/report.json --world-parameters \"{\\\"browser\\\": \\\"ie\\\"}\"",
    "test-edge-report": "./node_modules/.bin/cucumber-js  --format json:./reports/report.json --world-parameters \"{\\\"browser\\\": \\\"edge\\\"}\"",
    "test-firefox-report": "./node_modules/.bin/cucumber-js  --format json:./reports/report.json --world-parameters \"{\\\"browser\\\": \\\"firefox\\\"}\"",
    "test-safari-report": "./node_modules/.bin/cucumber-js  --format json:./reports/report.json --world-parameters \"{\\\"browser\\\": \\\"safari\\\"}\""
  },
  "dependencies": {
    "base64-img": "^1.0.4",
    "cucumber": "^4.2.1",
    "eslint": "^4.19.1",
    "npm": "^6.0.0",
    "testcafe": "^0.22.0"
  },
  "devDependencies": {
    "eslint-config-google": "^0.9.1"
  }
}
